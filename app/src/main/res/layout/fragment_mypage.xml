<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="411dp"
        android:layout_height="86dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <TextView
            android:id="@+id/welcome_text"
            android:layout_width="155dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"
            android:text="환영합니다!"
            android:textColor="#9370db"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </FrameLayout>

    <TextView
        android:id="@+id/community_text"
        android:layout_width="127dp"
        android:layout_height="33dp"
        android:text=" 커뮤니티에 작성한 글"
        tools:layout_editor_absoluteX="268dp"
        tools:layout_editor_absoluteY="358dp" />

    <TextView
        android:id="@+id/check_text"
        android:layout_width="111dp"
        android:layout_height="31dp"
        android:text=" 정보 기입 및 수정"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="358dp" />

    <TextView
        android:id="@+id/score_text"
        android:layout_width="142dp"
        android:layout_height="84dp"
        android:text="     상벌점 확인 및 기입"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="359dp" />

    <ImageView
        android:id="@+id/check"
        android:layout_width="107dp"
        android:layout_height="97dp"
        android:clickable="true"
        android:focusable="true"
        app:srcCompat="@drawable/check"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="261dp" />

    <ImageView
        android:id="@+id/score"
        android:layout_width="106dp"
        android:layout_height="96dp"
        android:clickable="true"
        android:focusable="true"
        app:srcCompat="@drawable/score"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="262dp" />

    <ImageView
        android:id="@+id/cummunity"
        android:layout_width="120dp"
        android:layout_height="101dp"
        android:clickable="true"
        android:focusable="true"
        app:srcCompat="@drawable/community"
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="258dp" />


</androidx.constraintlayout.widget.ConstraintLayout>